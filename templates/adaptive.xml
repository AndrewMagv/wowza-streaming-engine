						<SourceRectangle></SourceRectangle>
					</FrameSize>
				</Video>
				<Audio>
					<Codec>PassThru</Codec>
					<Bitrate>${SourceAudioBitrate}</Bitrate>
				</Audio>
			</Encode>
		</Encodes>
	</Transcode>
</Root>
